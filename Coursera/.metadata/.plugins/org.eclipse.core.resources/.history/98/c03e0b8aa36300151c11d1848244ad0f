

package week2;

import processing.core.*;

class GUISetup extends PApplet {
	private String URL = "http://www.lajollabythesea.com/uploads/media_items/sea-gull-at-windansea.0.194.1652.1267.440.338.c.jpg";
	private PImage bkgrndImage;
		
	public void setup() {
		
		bkgrndImage = loadImage(URL,"jpg");
		size(bkgrndImage.width,bkgrndImage.height);
	}
	
	public void draw(){
		image(bkgrndImage,0,0);
	}
}

public class HelloGUI extends GUISetup {
	System.out.println("this was run")
}
